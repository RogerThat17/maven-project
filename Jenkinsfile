pipeline {
    agent any
    stages {
        stage('Init') {
            steps {
                echo "Hello welcome to Jenkins world"
                echo "We are starting the testing"
            }
        }
        stage('Build') {
            steps {
                echo "We are building the project"
            }
        }
        stage("Deploy") {
            steps {
                echo "We are Deploying to UAT"
            }
        }
    }
}
